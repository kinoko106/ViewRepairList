             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ViewRepairList"
             xmlns:controls="http://schemes.grabacr.net/winfx/2015/kancolleviewer/controls"
             xmlns:contents="http://schemes.grabacr.net/winfx/2014/controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel Orientation="Vertical" VerticalAlignment="Top" HorizontalAlignment="Left">
        <Button x:Name="openWindow" Content="改修リスト" Width="100" Height="20" Click="open_repairList"/>
        <controls:SlotItemIcon Type="{Binding Icon1}"/>
    </StackPanel>
</UserControl>
